{
     "title": "AttomicAssetsApiClient",
     "styles" : {
            "website" : "./scss/page.scss"
     }, 
     "plugins": [  "github-buttons", 
                   "-sunchaser-logo",
                   "-fontsettings",
                   "theme-gestalt",
                   "-theme-default",
                   "styles-sass-fix",
                   "back-to-top-button",
                   "copy-code-button",
                   "cuav-chapters",
                   "heading-anchors"
                ],
  "pluginsConfig": { 
          "theme-gestalt": {
              "excludeDefaultStyles": true 
       },
        
       "github-buttons": {
          "buttons": [{
              "user": "liquiidio",
              "repo": "AtomicAssetsApiClient-Private",
              "type": "watch",
              "size": "large"
          }]
        },
        "sunchaser-logo": {
          "url": "https://avatars.githubusercontent.com/u/82725791?s=100",
          "style": "background: none;"
        }
     },
     "links": {
       "sidebar": {
          "Liquiid.io on GitHub": "https://github.com/liquiidio"
     },
     "sharing": {
        "all": ["facebook", "google", "twitter"],
        "facebook": true,
        "google": true,
        "twitter": true
     }
   },
   "pdf": {
    "margin": {
        "left": 56,
        "right": 56,
        "top": 56,
        "bottom": 56
    },
    "pageNumbers": false,
    "fontSize": 11,
    "fontFamily": "Roboto",
    "paperSize": "a4",
    "toc": true
  }
}
